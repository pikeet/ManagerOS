
<!--  View/MainWindow.xaml  -->
<Application
    x:Class="OSManager.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:OSManager"
    StartupUri="View/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Style/Fonts.xaml" />
                <ResourceDictionary Source="Style/Buttons.xaml" />
                <ResourceDictionary Source="Style/ListViewCustom/ListViewCustom.xaml" />
                <ResourceDictionary Source="Style/ProgressBarCustom.xaml" />
                <ResourceDictionary Source="Style/CheckBoxCustom.xaml" />
                <ResourceDictionary Source="Style/ComboBoxCustom.xaml" />
                <ResourceDictionary Source="Style/WindowStyleCustom.xaml" />
                <ResourceDictionary Source="Style/TabControlCustom.xaml" />
                <ResourceDictionary Source="Style/RichTextBoxCustom.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
